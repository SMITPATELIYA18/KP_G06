# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           controllers.IndexPageController.index(request: Request)
#POST     /                           controllers.IndexPageController.index(request: Request)
# An example controller showing how to use dependency injection
GET     /count                      controllers.CountController.count
# An example controller showing how to write asynchronous code
GET     /message                    controllers.AsyncController.message

GET     /issues						controllers.IssueController.getIssueStat(request: Request)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(file)

#GET     /time                       controllers.HomeController.currentTime

#GET     /hello/:message             controllers.HomeController.printHello(message:String)


# User profile routes
GET 	/sample-user-profile/:username		controllers.UserProfileController.getUserProfile(username: String)